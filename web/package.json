{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src/*.js --fix",
    "pre-commit": "lint-staged",
    "format": "prettier --write .",
    "prepare": "cd ../ && husky install ./web"
  },
  "lint-staged": {
    "*.(js|css)": [
      "prettier --write",
      "git add"
    ],
    ".js": [
      "npm run lint:fix",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.8.3",
    "antd": "^4.21.3",
    "axios": "^0.27.2",
    "i": "^0.3.7",
    "next": "12.1.6",
    "next-i18next": "^11.0.0",
    "npm": "^8.13.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.2",
    "react-webcam": "^7.0.1",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "2.7.1"
  }
}
